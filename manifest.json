{
  "manifest_version": 3,
  "name": "Xpath Generator",
  "description": "This is automatic Xpath Generator",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.PNG",
    "default_title": "Click on any element to get the xPath",
    "default_popup": "index.html"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ]
}